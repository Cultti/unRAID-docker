<?xml version="1.0" encoding="utf-8"?>
<Containers>
<Beta>True</Beta>
<Category>Tools:</Category>
  <Name>glances</Name>
  <Description>
	  Glances is a cross-platform curses-based system monitoring tool written in Python.
  </Description>
  <Overview>
	   Glances is a cross-platform curses-based system monitoring tool written in Python.
  </Overview>
  <Support>https://github.com/atribe/unRAID-docker</Support>
  <Registry>https://hub.docker.com/r/nicolargo/glances/</Registry>
  <GitHub>https://github.com/nicolargo/glances</GitHub>
  <Repository>nicolargo/glances</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>61209</HostPort>
        <ContainerPort>61209</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>61208</HostPort>
        <ContainerPort>61208</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <pid>host</pid>
  <Environment>
  </Environment>
  <Data>
    <Volume>
      <HostDir>/var/run/docker.sock</HostDir>
      <ContainerDir>/var/run/docker.sock</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:61208]/</WebUI>
  <Banner></Banner>
  <Icon></Icon>
  <ExtraParams></ExtraParams>
</Containers>
